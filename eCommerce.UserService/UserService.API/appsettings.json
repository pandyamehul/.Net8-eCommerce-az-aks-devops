{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "PostgresConnection": "Host=$POSTGRESDB_HOST;Port=$POSTGRESDB_PORT;Database=$POSTGRESDB_NAME;Username=$POSTGRESDB_USER;Password=$POSTGRESDB_PASSWORD"
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:9090"
      }
    }
  }
}